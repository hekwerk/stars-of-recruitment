{
  "name": "@stars-of-recruitment/client",
  "version": "1.0.0",
  "description": "Stars of Recruitment - Client",
  "author": "Hekwerk",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:client dev:functions",
    "build": "npm-run-all generate:client build:functions",
    "dev:client": "nuxt",
    "build:client": "nuxt generate",
    "start:client": "nuxt start",
    "generate:client": "nuxt generate",
    "lint:client": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test:client": "jest",
    "dev:functions": "netlify-lambda serve ./functions-ts",
    "build:functions": "netlify-lambda build ./functions-ts",
    "test:functions": "cross-env NODE_ENV=test jest ./functions-ts",
    "cleanup": "rimraf ./node_modules ./dist ./functions .nuxt"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/dotenv": "^1.4.1",
    "@types/aws-lambda": "^8.10.51",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/node-fetch": "^2.5.7",
    "@types/simple-oauth2": "^2.5.3",
    "@types/supertest": "^2.0.9",
    "express-bearer-token": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.12.2",
    "serverless-http": "^2.5.0",
    "simple-oauth2": "^3.4.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/preset-typescript": "^7.9.0",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.0.1",
    "netlify-lambda": "^1.6.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "vue-jest": "^3.0.5"
  }
}
